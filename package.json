{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./node_modules/expo/bin/cli start --dev-client",
    "start-web": "node ./node_modules/expo/bin/cli start --web",
    "start-web-dev": "DEBUG=expo* node ./node_modules/expo/bin/cli start --web",
    "lint": "node ./node_modules/expo/bin/cli lint",
    "android": "node ./node_modules/expo/bin/cli run:android",
    "ios": "node ./node_modules/expo/bin/cli run:ios",
    "start-tunnel": "node ./node_modules/expo/bin/cli start --dev-client --tunnel",
    "start-go": "node ./node_modules/expo/bin/cli start",
    "build:dev:ios": "node ./node_modules/eas-cli/bin/run build --profile development --platform ios",
    "build:dev:android": "node ./node_modules/eas-cli/bin/run build --profile development --platform android"
  },
  "dependencies": {
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@rork-ai/toolkit-sdk": "^0.2.51",
    "@tanstack/react-query": "^5.83.0",
    "expo": "~54.0.27",
    "expo-av": "~16.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.10",
    "expo-router": "~6.0.17",
    "expo-speech": "~14.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "lucide-react-native": "^0.475.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@types/react": "~19.1.10",
    "eslint": "^9.31.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2",
    "eas-cli": "^16.26.0"
  },
  "private": true
}
